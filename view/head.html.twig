<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<meta name="theme-color" content="#962d22">

<script src="https://cdn.jsdelivr.net/npm/jquery@3/dist/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/clipboard@2/dist/clipboard.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/cookieconsent@3/build/cookieconsent.min.js" defer></script>
<script src="https://cdn.jsdelivr.net/npm/leaflet@1/dist/leaflet.min.js" defer></script>
<script src="https://cdn.jsdelivr.net/npm/lazysizes@5/lazysizes.min.js" defer></script>
<script src="https://cdn.jsdelivr.net/npm/lazysizes@5/plugins/unveilhooks/ls.unveilhooks.min.js" defer></script>
<script src="https://www.google.com/recaptcha/api.js" async defer></script>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/combine/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css,npm/cookieconsent@3.1.1/build/cookieconsent.min.css,npm/hamburgers@1.1.3/dist/hamburgers.min.css">
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Fira+Sans:400,700,700i|Merriweather:400,400i,700&display=swap">
<link rel="stylesheet" href="{{ _x('/static/css/style.css') }}">
<link rel="stylesheet" href="{{ _x('/icons/css/all.css') }}">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/leaflet@1/dist/leaflet.min.css">

{% for language in _languages %}
    {% if language != _language %}
        <link rel="alternate" hreflang="{{ language }}" href="/{{ language }}{{ _uri|stripLanguage }}">
    {% endif %}
{% endfor %}